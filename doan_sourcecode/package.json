{
  "name": "vietnamgis-server",
  "version": "1.0.0",
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "set NODE_ENV=development&& nodemon server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.29",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.15.0",
    "compression": "1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "2.8.5",
    "cron": "^1.7.2",
    "csv-parse": "^4.4.6",
    "csvtojson": "^2.0.10",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.1",
    "hpp": "^0.2.2",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "mongoose": "^5.6.13",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "pg": "^7.12.1",
    "pug": "^2.0.4",
    "validator": "^11.1.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
